{
  "servers":
  [
    {
      "name": "py-ls",
      "filetype": ["py"],
      "path": "pylsp",
      "args": []
    },
    {
      "name": "vim-ls",
      "filetype": ["vim"],
      "path": "vim-language-server",
      "args": ["--stdio"]
    }, 
    {
      "name": "php-ls",
      "filetype": ["php"],
      "path": "intelephense",
      "args": ["--stdio"],
      "initializationOptions": {
          "licenceKey": ""
      },
      "workspaceConfig": {
      }
    },
    {
      "name": "vue-ls",
      "filetype": ["vue"],
      "path": "vue-language-server",
      "args": ["--stdio"],
      "initializationOptions": {
          "typescript": {
              "tsdk": ""
          },
          "vue": {
            "hybridMode": false
          }
       },
      "workspaceConfig": {
      }
    },
    {
      "name": "ts-ls",
      "filetype": ["js"],
      "path": "typescript-language-server",
      "args": ["--stdio"],
      "initializationOptions": {},
      "workspaceConfig": {}
    },
    {
      "filetype": ["json"],
      "path": "vscode-json-languageserver",
      "args": ["--stdio"],
      "initializationOptions": {},
      "workspaceConfig": {}
    },
    {
      "name": "bash-ls",
      "filetype": ["sh", "bash"],
      "path": "bash-language-server",
      "args": ["start"],
      "initializationOptions": {},
      "workspaceConfig": {}
    },
    {
      "name": "mscs-ls",
      "filetype": ["cs"],
      "path": ["dotnet"],
      "args": ["Microsoft.CodeAnalysis.LanguageServer.dll",
               "--logLevel", 
               "Information", 
               "--extensionLogDirectory", 
               "/tmp/ms-cs-ls", 
               "--stdio"],
      "initializationOptions": {},
      "workspaceConfig": {
        "projects": {
          "dotnet_binary_log_path": "C:/Temp/binlogs"
         }
      }
    },
    {
      "name": "rust-ls",
      "filetype": ["rs"],
      "path": "rust-analyzer",
      "args": [],
      "initializationOptions": {},
      "workspaceConfig": {}
    },
    {
      "name": "c-ls",
      "filetype": ["c", "cpp"],
      "path": "clangd",
      "args": ["--background-index", "--clang-tidy"],
      "initializationOptions": {},
      "workspaceConfig": {}
    }
  ]
}
