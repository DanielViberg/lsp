{
  "servers":
  [
    {
      "name": "vim-ls",
      "filetype": ["vim"],
      "path": "vim-language-server",
      "args": ["--stdio"]
    }, 
    {
      "name": "php-ls",
      "filetype": ["php"],
      "path": "intelephense",
      "args": ["--stdio"],
      "initializationOptions": {
          "licenceKey": ""
      },
      "workspaceConfig": {
      }
    },
    {
      "name": "vue-ls",
      "filetype": ["vue"],
      "path": "vue-language-server",
      "args": ["--stdio"],
      "initializationOptions": {
          "typescript": {
              "tsdk": ""
          },
          "vue": {
            "hybridMode": false
          }
       },
      "workspaceConfig": {
      }
    },
    {
      "name": "ts-ls",
      "filetype": ["js"],
      "path": "typescript-language-server",
      "args": ["--stdio"],
      "initializationOptions": {},
      "workspaceConfig": {}
    },
    {
      "filetype": ["json"],
      "path": "vscode-json-languageserver",
      "args": ["--stdio"],
      "initializationOptions": {},
      "workspaceConfig": {}
    },
    {
      "name": "bash-ls",
      "filetype": ["sh", "bash"],
      "path": "bash-language-server",
      "args": ["start"],
      "initializationOptions": {},
      "workspaceConfig": {}
    },
    {
      "name": "csharp-ls",
      "filetype": ["cs"],
      "path": "csharp-ls",
      "initializationOptions": {},
      "workspaceConfig": {}
    },
    {
      "name": "rust-ls",
      "filetype": ["rs"],
      "path": "rust-analyzer",
      "args": [],
      "initializationOptions": {},
      "workspaceConfig": {}
    },
    {
      "name": "c-ls",
      "filetype": ["c", "cpp"],
      "path": "clangd",
      "args": ["--background-index", "--clang-tidy"],
      "initializationOptions": {},
      "workspaceConfig": {}
    }
  ]
}
